path:display/my_conference
input:
    必须先登录
    method:GET
output:
    输出该单位用户或其子用户的全部会议
    message: 'success'/'no conference'/'invalid organization user'
    data: [{
                'conference_id':xxx,
                'conference_title':xxx,
            },{
                'conference_id':xxx,
                'conference_title':xxx,
            }]

path:display/my_submission
input:
    必须先登录
    method:GET
output:
    输出该用户的所有提交论文列表
    message: 'success'/'no submissions'
    data: [{
                'submission_id': xxx,
                'paper_name': xxx,
            },{
                'submission_id': xxx,
                'paper_name': xxx,
            }]

path:display/conference/<int:id>/papers
input:
    必须先登录
    id:会议id
    method:GET
output:
    输出一个会议的所有提交论文列表
    message: 'success'/'no submissions'/'no conference'
    data: [{
                'submission_id': xxx,
                'paper_name': xxx,
            },{
                'submission_id': xxx,
                'paper_name': xxx,
            }]

path:display/conference/<int:id>/activities
input:
    必须先登录
    id:会议id
    method:GET
output:
    输出一个会议的所有活动
    message: 'success'/'no activities'/'no conference'
    data: [{
                'activity_id',
                'activity_name',活动名称
                'start_time'
                'end_time'
                'place': 活动地点
            },{
                'activity_id',
                'activity_name',活动名称
                'start_time'
                'end_time'
                'place': 活动地点
            }]